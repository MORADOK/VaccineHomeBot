# VCHome Hospital Environment Configuration

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Application Configuration
VITE_APP_NAME=VCHome Hospital
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production

# Domain Configuration
VITE_DEFAULT_DOMAIN=vchomehospital.com
VITE_API_BASE_URL=https://api.vchomehospital.com

# Feature Flags
VITE_ENABLE_DESKTOP_MODE=true
VITE_ENABLE_DOMAIN_MANAGEMENT=true
VITE_ENABLE_ADMIN_FEATURES=true

# Security Configuration
VITE_ADMIN_PASSWORD_MIN_LENGTH=8
VITE_SESSION_TIMEOUT=3600000

# Google Sheets Integration (Optional)
VITE_GOOGLE_SHEETS_API_KEY=your_google_sheets_api_key
VITE_GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id

# LINE Bot Integration (Optional)
VITE_LINE_CHANNEL_ACCESS_TOKEN=your_line_channel_access_token
VITE_LINE_CHANNEL_SECRET=your_line_channel_secret