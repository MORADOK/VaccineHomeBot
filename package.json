{
  "name": "vchome-hospital-desktop",
  "private": true,
  "version": "1.0.8",
  "description": "VCHome Hospital Management System - Desktop Application",
  "author": "VCHome Hospital",
  "main": "public/electron-final-fix.js",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently -k \"vite --config vite.config.electron.ts\" \"wait-on http://localhost:5173 && electron .\"",
    "build": "vite build --config vite.config.electron.ts",
    "build:web": "vite build --config vite.config.electron.ts",
    "build:github": "vite build --mode github-pages --config vite.config.electron.ts",
    "build:dev": "vite build --mode development --config vite.config.electron.ts",
    "lint": "eslint .",
    "preview": "vite preview --config vite.config.electron.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "electron": "electron .",
    "electron-prod": "cross-env NODE_ENV=production electron .",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "dist-win": "npm run build && electron-builder --win --publish=never",
    "dist-mac": "npm run build && electron-builder --mac --publish=never",
    "dist-linux": "npm run build && electron-builder --linux --publish=never",
    "pack": "npm run build && electron-builder --dir",
    "publish-win": "npm run build && electron-builder --win --publish=always",
    "publish-mac": "npm run build && electron-builder --mac --publish=always",
    "publish-linux": "npm run build && electron-builder --linux --publish=always",
    "verify-appointments": "node scripts/verify-appointment-dates.js",
    "fix-appointments": "node scripts/fix-appointment-dates.js",
    "list-vaccines": "node scripts/list-vaccines.js",
    "recalculate-appointments": "node scripts/recalculate-all-appointments.js",
    "publish-draft": "npm run build && electron-builder --win --publish=onTagOrDraft",
    "verify-publish": "node scripts/verify-publish-setup.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.54.0",
    "@tanstack/react-query": "^5.56.2",
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-log": "^2.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.3",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "embla-carousel-react": "^8.3.0",
    "input-otp": "^1.2.4",
    "lovable-tagger": "^1.1.10",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "react-router-dom": "^6.26.2",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "electron": "^38.4.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.2"
  },
  "build": {
    "appId": "com.vchomehospital.vaccine-app",
    "productName": "VCHome Hospital",
    "copyright": "Copyright Â© 2024 VCHome Hospital",
    "directories": {
      "output": "release"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "MORADOK",
        "repo": "VaccineHomeBot"
      }
    ],
    "files": [
      "dist-electron/**",
      "public/electron-final-fix.js",
      "public/preload.js",
      "electron/assets/**"
    ],
    "extraResources": [
      {
        "from": "electron/assets/icons/win/app.ico",
        "to": "app.ico"
      }
    ],
    "mac": {
      "category": "public.app-category.medical",
      "icon": "electron/assets/icons/mac/app.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "win": {
      "verifyUpdateCodeSignature": false,
      "signAndEditExecutable": false,
      "icon": "electron/assets/icons/win/app.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "electron/assets/icons/png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Office"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "VCHome Hospital"
    },
    "portable": {
      "artifactName": "VCHome-Hospital-Portable.exe"
    }
  }
}
